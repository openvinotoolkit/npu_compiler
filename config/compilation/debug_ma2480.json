{
    "adapt":
    {
        "Singular" :
        [
            {
                "name" : "GenerateDot",
                "output" : "original_model.dot"
            },
            "ConvolutionDilation",
            "FuseBatchNorm",
            "FuseBias",
            "FuseScale",
            "FuseBias",
            "FuseScale",
            "FuseBias",
            "FullyConnectedAsConv2D",
            "RemoveDropOut",
            {
                "name" : "GenerateDot",
                "output" : "adapt.dot"
            }
        ]
    },

    "finalize":
    {
        "Singular" :
        [
            "AlignConstOrder",
            "MarkHardwareOperations",
            "FuseReluMX",
            "AddConversionLayers",
            "MyriadXPaddings",
            "OptimizePoolings",
            "OptimizeConvolutions",
            "SplitsOverH",
            {
                "name" : "ScaleFission",
                "scalefactors":
                {
                    "conv2d_2": 7.6,
                    "conv2d_4": 8.0
                }
            },
            "FormatMXWeights",
            "ArrangeLinearExecution",
            "AllocatePopulatedTensors",
            "AllocateUnpopulatedTensors",
            "AllocateInputOutputTensors",
            {
                "name" : "GenerateDot",
                "output" : "finalize.dot"
            }
        ]
    },

    "serialize":
    {
        "Singular" :
        [
            "PopulateSerialFields",
            {
                "name" : "GenerateBlob",
                "fileName" : "cpp.blob",
                "enableFileOutput" : true,
                "enableRAMOutput" : false
            }
        ]
    },

    "validate":
    {
        "Singular" :
        [
            "CheckTensors"
        ]
    },

    "root" :
    {
        "Singular" :
        [
            "validate",
            "adapt",
            "finalize",
            "serialize"
        ],
        "Recurrent" :
        [
            "validate"
        ]
    }
